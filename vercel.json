{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "scripts.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/styles.css",
        "dest": "/styles.css"
      },
      {
        "src": "/scripts.js",
        "dest": "/scripts.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  